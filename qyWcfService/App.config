<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
          <endpointBehaviors>
              <behavior name="Artech.DuplexServices.Services.CalculatorService">
                    <endpoint address="net.tcp://127.0.0.1:9999/CalculatorService"
                binding="netTcpBinding" contract="Artech.DuplexServices.Contracts.ICalculator" />
              </behavior>
          </endpointBehaviors>
        </behaviors>

      <bindings>
        <wsDualHttpBinding>
          <binding name="WSDualHttpBinding_ISendMessageService" />
        </wsDualHttpBinding>
      </bindings>
      <client>
        <endpoint address="http://localhost:24612/SendMessageService.svc"
          binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ISendMessageService"
          contract="ServiceReference1.ISendMessageService" name="WSDualHttpBinding_ISendMessageService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
      </client>
    </system.serviceModel>
</configuration>